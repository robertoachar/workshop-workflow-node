.token.comment {
  font-style italic
}

.token.comment,
.token.prolog,
.token.doctype,
.token.cdata {
	color: #00a8ff;
}

.token.punctuation {
	color: #f5f6fa;
}

.namespace {
	opacity: .7;
}

.token.attr-name,
.token.boolean,
.token.constant,
.token.deleted,
.token.function,
.token.number,
.token.property,
.token.symbol,
.token.tag {
	color: #ffc600;
}

.token.selector,
.token.string,
.token.char,
.token.builtin,
.token.inserted {
	color: #44bd32;
}

.token.operator,
.token.entity,
.token.url,
.language-css .token.string,
.style .token.string {
	color: #e1b12c;
}

.token.atrule,
.token.attr-value,
.token.keyword {
	color: #f5f6fa;
}

.token.class-name {
	color: #e84118;
}

.token.regex,
.token.important,
.token.variable {
	color: #e90;
}

.token.important,
.token.bold {
	font-weight: bold;
}
.token.italic {
	font-style: italic;
}

.token.entity {
	cursor: help;
}
